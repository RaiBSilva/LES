@model LES.Models.ViewModel.Admin.AdminLoginModel

@{
    ViewData["Title"] = "Livros";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Login</h1>

<div id="divConteudo" class="col-md-6">
    <form asp-action="Login" asp-controller="Conta">
        <div id="divPartial">
            <partial name="../Admin/PartialViews/_LoginFormPartial.cshtml" for="@Model">
        </div>
        <div id="divBotao" class="form-group">
            <input type="submit" value="Cadastrar" class="btn btn-primary" id="btn" />
        </div>
    </form>

</div>