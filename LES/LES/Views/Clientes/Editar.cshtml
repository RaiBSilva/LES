@model LES.Models.ViewModel.ClienteCadastro


@{
    ViewData["Title"] = "Editar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Editar Informações Pessoais</h1>

<form asp-action="Editar" asp-controller="Clientes">

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <h2>Informações Pessoais</h2>
    <div class=" col-md-6">
        @Html.Partial("_InfoClientePartial", Model)

        <div class="form-group">
            <input type="submit" value="Editar" class="btn btn-primary" id="btn" />
        </div>
    </div>


</form>

@section Scripts{
    <script src="~/js/PrevenirCadastro.js"></script>
    <script src="~/js/ValRequired.js"></script>
}
