@model IList<DetalhesEnderecoModel>


    <div>
        <div class="row">
            <h2 class="font-weight-light col-sm-12">
                Endereços
            </h2>
        </div>
        <div class="row">
            @foreach (var end in Model)
            {
                <div class="col-lg-6 my-1 card">
                    <partial name="../PartialViews/_DlEnderecoPartial.cshtml" for="@end" />
                    <dl>
                        <dd class="row justify-content-start mx-1">
                            <button class="btn btn-secondary botaoEndereco col-6 mr-3" type="button" data-toggle="modal" data-target="#myModal" value="@end.Id">
                                Editar este endereço
                            </button>
                            @if (Model.Count > 1)
                            {
                                <button class="btn btn-danger botaoRemoverEndereco col-2 mx-3" type="button" data-toggle="modal" data-target="#myModal" value="@end.Id">
                                    &times;
                                </button>
                            }

                            @if (!end.EPreferencial)
                            {
                                <button class="btn btn-outline-warning col-2 ml-3" type="button" value="@end.Id">
                                    &#9733;
                                </button>
                            }
                        </dd>
                    </dl>
                </div>
            }
        </div>
        <div class="row justify-content-center">
            <nav aria-label="Paginação" class="mt-3">
                <ul class="pagination">
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                            <span class="sr-only">Next</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>