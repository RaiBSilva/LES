@model IList<DetalhesEnderecoModel>


    <div>
        <div class="row">
            <h2 class="font-weight-light col-sm-12">
                Endereços
            </h2>
        </div>
        <div class="row">
            @foreach (var end in Model)
            {
                <div class="col-lg-6 card">
                    <partial name="../PartialViews/_DlEnderecoPartial.cshtml" for="@end" />
                    <dl>
                        <dd>
                            <button class="btn btn-secondary botaoEndereco" type="button" data-toggle="modal" data-target="#myModal" value="@end.Id">
                                Editar este endereço
                            </button>
                            <button class="btn btn-danger botaoRemoverEndereco" type="button" data-toggle="modal" data-target="#myModal" value="@end.Id">
                                &times;
                            </button>
                        </dd>
                    </dl>
                </div>
            }
        </div>
    </div>