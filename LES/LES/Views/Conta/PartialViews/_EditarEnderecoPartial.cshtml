@model DetalhesEnderecoModel

    <div class="modal-content">

        <!-- Modal content-->
        <div class="modal-header">
            <h4 class="modal-title">Editar Endereço</h4>
            <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
            <form asp-action="EditarEndereco" asp-controller="Conta">

                <div hidden>
                    <label asp-for="Id" class="control-label"></label>
                    <input asp-for="Id" class="form-control required" readonly />
                    <span asp-validation-for="Id" class="text-danger required"></span>
                </div>

                <div>
                    <label asp-for="NomeEndereco" class="control-label"></label>
                    <input asp-for="NomeEndereco" class="form-control required" />
                    <span asp-validation-for="NomeEndereco" class="text-danger required"></span>
                </div>

                <partial name="../PartialViews/_EnderecoPartial.cshtml" for="@Model" />

                <div>
                    <label asp-for="EPreferencial" class="control-label"></label>
                    <input asp-for="EPreferencial" class="form-control required" />
                    <span asp-validation-for="EPreferencial" class="text-danger required"></span>
                </div>

                <div>
                    <label asp-for="ECobranca" class="control-label"></label>
                    <input asp-for="ECobranca" class="form-control required" />
                    <span asp-validation-for="ECobranca" class="text-danger required"></span>
                </div>

                <div>
                    <label asp-for="EEntrega" class="control-label"></label>
                    <input asp-for="EEntrega" class="form-control required" />
                    <span asp-validation-for="EEntrega" class="text-danger required"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Editar" class="btn btn-primary" id="btn" />
                </div>

            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
        </div>

    </div>